{
  "name": "react-muuri",
  "version": "0.2.0",
  "description": "A React implementation of Muuri",
  "author": "Matt Tischler",
  "license": "MIT",
  "repository": "mjtischler/react-muuri",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "rollup -c",
    "cucumber": "cucumber-js --require cucumber.config.js cucumber/features/ -r cucumber/steps/",
